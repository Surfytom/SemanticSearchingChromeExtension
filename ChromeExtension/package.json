{
  "name": "testchromeextension",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "pack": "node pack.js",
    "repack": "npm run build && npm run pack",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\"",
    "test": "jest testing/test.js"
  },
  "devDependencies": {
    "adm-zip": "^0.5.10",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.5",
    "prettier": "^2.8.7",
    "webpack": "^5.78.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "jest": "^29.5.0",
    "node": "^19.8.1",
    "os-browserify": "^0.3.0",
    "puppeteer": "^19.9.1",
    "socket-io": "^1.0.0",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.6.1",
    "weaviate-ts-client": "^1.0.0"
  },
  "browser": {
    "child_process": false
  }
}
